pipeline {
    agent any
    stages {
        stage("Build"){
            steps{
                sh 'echo "Hello from Build"'
            }
        }
        stage("Test"){
            steps{
                sh 'echo "Hello from Test"'
            }
        }
        stage("Deploy"){
            steps{
                sh 'echo "Hello from Deploy"'
            }
        }
    }
}